{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "eslint.validate": [
    "javascript",
    "vue"
  ],
  "vetur.validation.template": false,
  "conventionalCommits.scopes": [
    "db",
    "ui",
    "model",
    "page",
    "routes",
    "security"
  ],
  "bladeFormatter.format.indentSize": 2
}
